{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",

    "paths": {
      "@/": ["./"]
    }
  },
  "include": [
    "/*.ts",
    "/*.tsx",
    "/*.jsx",
    ".expo/types//*.ts",
    "expo-env.d.ts",
    "app/(tabs)/index.jsx",
    ".expo/types/**/*.ts"
  ]
}
