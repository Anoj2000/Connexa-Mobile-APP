{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.js",
    "**/*.jsx",
    "app/(tabs)/index.jsx"
  ],
  "exclude": [
    "node_modules",
    "venv",
    "**/*.ts",
    "**/*.tsx"
  ]
}
